<Window x:Class="WPFAdressBpk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFAdressBpk"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Text="Förnamn" />
            <TextBox x:Name="tb_FirstName" Padding="5" />

            <TextBlock Text="Efternamn" Margin="0 10 0 0" />
            <TextBox x:Name="tb_LastName" Padding="5" />

            <Button Content="VISA" Margin="0 30 0 20" Padding="10" />
        </StackPanel>
    </Grid>
</Window>
